pollutantmean <- function(directory, pollutant, id = 1:332, removeNA = TRUE) {} {
data <- numeric(0) #create empty vector for temp output
loop <- length(id)
files <- dir(directory)
if(pollutant == "sulfur"){
x <- 2
} else {
x <- 3
}
for(i in 1:loop{
foo <- read.table(files[i],sep=",",header=TRUE)
foo <- foo([,x])
poo = cbind(data,foo)
rm(foo)
}
means <- mean(poo, na.rm=removeNA))
}
clear
rm data
rm(data, files, i, id, loop)
rm(directory)
}
